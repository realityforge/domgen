  protected void log( @javax.annotation.Nonnull final iris.syncrecord.server.service.SynchronizationRecorder recorder, final String message )
  {
    LOG.info( message );
    recorder.addMessage( message );
  }

  protected void insertMasterDataSourceIfRequired( @javax.annotation.Nonnull final String dataSource )
  {
    executeUpdate( "INSERT INTO Master.tblMappingSource(Code) " +
                   "  SELECT '" + dataSource + "' " +
                   "  WHERE 0 = (SELECT COUNT(*) FROM Master.tblMappingSource WHERE Code = '" + dataSource + "')" );
